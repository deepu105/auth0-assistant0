{
  "name": "auth-0-ai-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "dotenv -e .env.vivaldi -- docusaurus start",
    "build": "npm run build:vivaldi",
    "build:vivaldi": "dotenv -e .env.vivaldi -- npm run builder",
    "builder": "docusaurus build --out-dir ../auth0-ai-landing/public/docs_out",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "dotenv -e .env.vivaldi docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.1.0",
    "@docusaurus/core": "3.7.0",
    "@docusaurus/preset-classic": "3.7.0",
    "@easyops-cn/docusaurus-search-local": "^0.48.5",
    "@mdx-js/react": "^3.0.0",
    "auth0": "^4.20.0",
    "clsx": "^2.0.0",
    "js-cookie": "^3.0.5",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-aria": "^3.38.1",
    "react-dom": "^19.0.0",
    "react-stately": "^3.36.1",
    "swr": "^2.3.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.7.0",
    "@docusaurus/tsconfig": "3.7.0",
    "@docusaurus/types": "3.7.0",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
